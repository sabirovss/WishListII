<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="javaguru.app">
  <class name="Wish" table="WISH">
    <id column="WID" name="WID" type="java.lang.Integer">
      <generator class="increment"/>
    </id>
    <property column="MYWISH" name="myWish" type="java.lang.String"/>
    <property column="PRICE" name="price" type="java.lang.String"/>
    <property column="SHOP" name="shop" type="java.lang.String"/>
  </class>
</hibernate-mapping>
